<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
 
    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="fengjr_ipDataFileService_consumer"  />
 
    <!-- 使用zookeeper注册中心暴露服务地址 -->
    <dubbo:registry  protocol="zookeeper" address="bzk1.fengjr.inc:2181,bzk2.fengjr.inc:2181,bzk3.fengjr.inc:2181" />
    
	<!-- IP查询服务接口-->
    <dubbo:reference id="ipDataFileService" interface="com.feng.ipcenter.service.IPDataFileService" />
    <dubbo:reference id="iPhoneLocationService" interface="com.feng.ipcenter.service.IPhoneLocationService" />

<!-- 	<dubbo:consumer filter="metricsFilter" /> -->

</beans>