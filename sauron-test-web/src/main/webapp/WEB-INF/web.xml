<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee   http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
     
<!--      <listener> -->
<!--         <listener-class>com.feng.sauron.client.listener.SauronLoadTimeWeavingListener</listener-class> -->
<!--     </listener> -->
     
     <listener>
        <listener-class>
            org.springframework.web.context.ContextLoaderListener
        </listener-class>
    </listener>
     
     
     <context-param>
        <param-name>contextConfigLocation</param-name>
<!--         <param-value>classpath*:/*-config.xml,classpath*:/upm.xml</param-value> -->
        <param-value>classpath*:/*-config.xml</param-value>
    </context-param>
     
    <servlet>
        <servlet-name>rocketmq</servlet-name>
        <servlet-class>com.fengjr.sauron.test.web.RocketMQServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rocketmq</servlet-name>
        <url-pattern>/rocketmq</url-pattern>
    </servlet-mapping>

 

   
    
    <servlet>
        <servlet-name>Spring MVC Dispatcher Servlet</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <init-param>
            <param-name>contextConfigLocation</param-name>
            <param-value>classpath*:/jsp-servlet.xml</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Spring MVC Dispatcher Servlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

</web-app>